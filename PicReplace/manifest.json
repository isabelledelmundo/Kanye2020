{
  "manifest_version": 2,

  "name": "Kanye",
  "version": "0.1.0",
  "description": "Lateral blog post demo extension",
  "permissions": ["*://*/*"],


  "browser_action": {
    "default_icon": "icon1_19x19.png"
  },

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["*://*/*"],
    "run_at": "document_end"
  }]

}

{
  "manifest_version": 2,

  "name": "Kanye",
  "version": "0.1.0",
  "description": "changes all pictures to cats and all text of Donald Trump to Kanye 2020",
  "permissions": ["*://*/*"],


  "browser_action": {
    "default_icon": "icon1_19x19.png",
    "default_title": "Make America Great Again"
  },

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["*://*/*"],
    "run_at": "document_end"
  }]

}
